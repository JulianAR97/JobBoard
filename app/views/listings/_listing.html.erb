<div class="show-head" style="inline">
  <h3><%= show_listing(listing)%>
  <% if params[:user_id] %>
    <%= link_to "Edit", edit_user_listing_path(current_user, listing), type: "button", class: "btn y-bg" %>
    <%= link_to "Delete", user_listing_path(current_user, listing), type:'button', class:"btn r-bg", method:'delete'%>
  <% end %>
  </h3>
</div>
<div class="tags">
  <% listing.tag_names.each do |tag| %>
    <%# tag: tag allows a param to be added%>
    <span class="no-link-style <%= get_tag_class(tag) %>"><%= link_to tag, listings_path(tag: tag)%></span>
  <% end %>
</div>

<% if show_content %>
  <%= listing.description %>
  <br>
<% end %>



<div class="tags">
  <span class="pay"><%= listing.display_pay %></span>
</div>

<div class="tags">
  <span class="no-link-style <%= listing.skill_level %>" ><%= link_to listing.skill_level, listings_path(skill_level: listing.skill_level) %></span>
</div>

<div class="tags">
  <span class="creator"><%= listing.creator_email %></span>
</div>










